{
  "name": "golfpad",
  "version": "0.1.0",
  "private": true,
  "description": "100% on-chain golf fun.",
  "keywords": [
    "Golf",
    "SocialFi",
    "ICP"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prebuild": "npm run generate",
    "generate": "dfx generate && node ./update.types.mjs && npm run format"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.6.3",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.44.1",
    "sass": "^1.79.4",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.4",
    "svelte-fa": "^4.0.3",
    "svelte-preprocess": "^6.0.3",
    "svelte-select": "^5.8.3",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^2.1.2",
    "@dfinity/auth-client": "^2.1.2",
    "@dfinity/candid": "^2.1.2",
    "@dfinity/gix-components": "^4.7.0",
    "@dfinity/nns": "^7.0.0",
    "@dfinity/nns-proto": "^2.0.1",
    "@dfinity/principal": "^2.1.2",
    "@dfinity/sns": "^3.2.1",
    "@dfinity/utils": "^2.5.1",
    "esbuild": "^0.14.0",
    "lodash-es": "^4.17.21",
    "node-modules-polyfill": "^0.1.4",
    "postcss": "^8.4.47",
    "svelte-flag-icons": "^1.0.3",
    "tailwindcss": "^3.4.13"
  }
}
